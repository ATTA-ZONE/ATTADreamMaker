{
  "manifest_version": 2,
  "name":"new-tab-page",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "pages.html"
  },
  "browser_action": {
    "default_popup": "pages.html",
    "default_title": "鼠标放上去时候的名称",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
